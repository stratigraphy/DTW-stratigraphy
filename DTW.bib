@article{wichernDecodingDeepTimeRhythms2024,
  title = {Decoding {{Deep-Time Rhythms}}: {{Probing}} the {{Limit}} of {{Stratigraphic Correlation}} in the {{Time-Specific Facies}} of the {{Late Devonian Usseln Limestone}} ({{Rhenish Massif}}, {{Germany}})},
  author = {Wichern, N. M.A. and Bialik, O. M. and Nohl, T. and Becker, R. T. and De Vleeschouwer, D.},
